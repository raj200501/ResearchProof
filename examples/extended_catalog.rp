# Extended catalog of small definitional proofs.

theorem plus_0_0 : plus Z Z = Z
proof Refl

theorem plus_0_1 : plus Z (S Z) = S Z
proof Refl

theorem plus_1_0 : plus (S Z) Z = S Z
proof Refl

theorem plus_1_1 : plus (S Z) (S Z) = S (S Z)
proof Refl

theorem plus_2_1 : plus (S (S Z)) (S Z) = S (S (S Z))
proof Refl

theorem plus_2_0 : plus (S (S Z)) Z = S (S Z)
proof Refl

theorem mult_0_0 : mult Z Z = Z
