theorem list_concat_two : concat (Cons (Cons Z Nil) (Cons (Cons (S Z) Nil) Nil)) = Cons Z (Cons (S Z) Nil)
proof Refl

theorem list_replicate_zero : replicate Z Z = Nil
proof Refl

theorem list_replicate_one : replicate (S Z) Z = Cons Z Nil
proof Refl

theorem list_filter_true : filter (\_ => True) (Cons Z Nil) = Cons Z Nil
proof Refl

theorem list_filter_false : filter (\_ => False) (Cons Z Nil) = Nil
proof Refl

