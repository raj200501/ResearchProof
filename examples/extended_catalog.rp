theorem mult_1_0 : mult (S Z) Z = Z
proof Refl

theorem mult_1_1 : mult (S Z) (S Z) = S Z
proof Refl

theorem mult_2_1 : mult (S (S Z)) (S Z) = S (S Z)
proof Refl

theorem mult_2_2 : mult (S (S Z)) (S (S Z)) = plus (S (S Z)) (S (S Z))
proof Refl

theorem pow_2_0 : pow (S (S Z)) Z = S Z
proof Refl

theorem pow_2_1 : pow (S (S Z)) (S Z) = S (S Z)
proof Refl

theorem pow_2_2 : pow (S (S Z)) (S (S Z)) = mult (S (S Z)) (S (S Z))
proof Refl

theorem double_0 : double Z = Z
proof Refl

theorem double_1 : double (S Z) = S (S Z)
proof Refl

theorem double_2 : double (S (S Z)) = S (S (S (S Z)))
