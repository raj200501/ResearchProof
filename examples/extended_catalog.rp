theorem list_append_two : append (Cons Z Nil) (Cons (S Z) Nil) = Cons Z (Cons (S Z) Nil)
proof Refl

theorem list_append_three : append (Cons Z (Cons (S Z) Nil)) (Cons (S (S Z)) Nil) = Cons Z (Cons (S Z) (Cons (S (S Z)) Nil))
